#include<iostream>
#include<cstdlib>
#include<ctime>
using namespace std;
int main()
{
    char arr[]="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890!@#$%^&*()_";
    int y,u;
    cout<<"Please enter the number of different passwords: ";
    cin>>y;
    while (y--)
    {
        cout << "Please enter the length of the desired password: \n";
        cin >> u;
        srand(time(0));
        for (int m = 1; m <= u; m++)
        {
            cout << arr[rand() % 74];
        }
        cout << "\n";
    }
    return 0;
}
